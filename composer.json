  },
  "repositories": {
    "amqp": {
      "type": "vcs",
      "url": "git@bitbucket.org:alldigitalrewards/amqp.git"
    },
    "rap": {
      "type": "vcs",
      "url": "git@bitbucket.org:alldigitalrewards/rap.git"
    },
    "catalog-wrapper": {
      "type": "vcs",
      "url": "git@bitbucket.org:alldigitalrewards/catalog-wrapper.git"
    }
  }
}
