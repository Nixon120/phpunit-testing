    "particle/validator": "^2.3",
    "alldigitalrewards/catalog-wrapper": "^1.0.2",
    "superbalist/flysystem-google-storage": "^6.0",
    "jojihere/monolog-fluent": "^1.0",
    "ramsey/uuid": "^3.7",
    "phpoffice/phpspreadsheet": "^1.2",
    "dompdf/dompdf": "^0.8.2",
    "cache/redis-adapter": "^1.0"
